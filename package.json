{
  "name": "polofield",
  "version": "0.0.0",
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.4",
    "@cloudflare/workers-types": "^4.20241205.0",
    "@miniflare/core": "^2.14.4",
    "@miniflare/html-rewriter": "^2.14.4",
    "@slack/bolt": "^4.1.1",
    "@types/suncalc": "^1.9.2",
    "discord-api-types": "^0.37.111",
    "fetch-cookie": "^3.0.1",
    "hono": "^4.6.13",
    "node-xlsx": "^0.24.0",
    "prettier": "^3.4.2",
    "serverless-cloudflare-workers": "^1.2.0",
    "suncalc": "^1.9.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wrangler": "^3.94.0"
  },
  "private": true,
  "scripts": {
    "start": "export NO_D1_WARNING=true && wrangler dev",
    "deploy": "export NO_D1_WARNING=true && wrangler deploy",
    "wrangler": "export NO_D1_WARNING=true && wrangler",
    "test": "vitest --run",
    "test:network": "TEST_NETWORK=1 npm run test",
    "vitest": "vitest",
    "check": "tsc --noEmit --skipLibCheck",
    "check-updates": "npx npm-check-updates",
    "format": "prettier --write 'src/**/*.{js,ts,css,json,md}'",
    "scrape": "NODE_OPTIONS=--no-warnings=ExperimentalWarning tsx scripts/scrape.ts"
  },
  "prettier": {}
}
